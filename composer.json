        "yiisoft/yii2": "~2.0.14",
        "yiisoft/yii2-bootstrap4": "~2.0.6",
        "yiisoft/yii2-swiftmailer": "~2.0.0",
        "yiisoft/yii2-jui": "~2.0.0",
        "bower-asset/bootstrap": "4.*",
        "bower-asset/bootstrap-rtl": "*",
        "bower-asset/font-awesome": "5.*",
        "bower-asset/select2": "~4.0",
        "gholizadeh/yii2-summernote": "dev-master",
        "cranky4/change-log-behavior": "1.0.6",
        "phpoffice/phpspreadsheet": "dev-master",
        "oxyaction/yii2-polymorphic-relation-behavior": "dev-master"
    },
    "require-dev": {
